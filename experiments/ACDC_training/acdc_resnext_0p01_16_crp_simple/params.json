{
  "model_id": "ResNeXt_DeepLabV3_plus",
  "dataset": "ACDC_training",

"learning_rate": 0.01,
"batch_size": 16,
"n_epochs": 300,
"default_height": 224,
"default_width": 224,
"roi_height": 128,
"roi_width": 224,
"k_split": 5,
"norm_type": "per_dataset",
"data_augmentation": "simple",
"optimizer": "adam",
"weight_decay": 0.0001,
"use_aspp": false,
"lr_decay": "divide",
"first_decay": 180,
"second_decay": 270,
"roi_crop": "no_roi",
"replace_stride": [false, false, true],
"relative_roi_perturbation": true,
"random_crop_scale": [0.33, 1],
"random_crop_ratio": [0.9, 1.1]
}
