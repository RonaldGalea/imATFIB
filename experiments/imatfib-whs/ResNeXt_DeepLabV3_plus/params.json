{
"model_id": "ResNeXt_DeepLabV3_plus",
"dataset": "imatfib-whs",

"learning_rate": 0.001,
"batch_size": 12,
"n_epochs": 300,
"default_height": 256,
"default_width": 256,
"roi_height": 128,
"roi_width": 128,
"k_split": 5,
"norm_type": "per_dataset",
"data_augmentation": "heavy",
"optimizer": "adam",
"weight_decay": 0.0001,
"use_aspp": false,
"lr_decay": "divide",
"first_decay": 180,
"second_decay": 270,
"roi_crop": "relative_roi",
"replace_stride": [false, true, true],
"relative_roi_perturbation": true,
"random_crop_scale": [1, 1],
"random_crop_ratio": [0.9, 1.1]
}
